{
  "name": "linkedin-cv-matcher",
  "version": "1.0.0",
  "description": "LinkedIn CV-Based Connection Finder - Find relevant connections for coffee chats",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:all": "npm run test && npm run test:e2e",
    "test:clean": "rm -rf coverage .vitest",
    "lint": "eslint src tests --ext .js",
    "lint:fix": "eslint src tests --ext .js --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\" \"tests/**/*.js\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "mammoth": "^1.6.0",
    "pdfjs-dist": "^4.0.379"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "linkedin",
    "cv",
    "resume",
    "networking",
    "career",
    "job-search",
    "connections"
  ],
  "author": "",
  "license": "MIT"
}
